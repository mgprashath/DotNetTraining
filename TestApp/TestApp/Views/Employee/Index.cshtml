@model IEnumerable<TestApp.Models.Employee>

    @{
        ViewBag.Title = "Employee List";
    }

    <h2>Employee List</h2>
    <div>@Html.ActionLink("Create","Create","Employee",null,new {@class = "nav-link"})</div>
    <table>
        <thead>
            <tr>
                <th>ID</th>
                <th>Name</th>
                <th>Position</th>
                <th>DOM</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var employee in Model)
            {
                <tr>
                    <td>@employee.EmployeeId</td>
                    <td>@employee.Name</td>
                    <td>@employee.Position</td>
                    <td>@employee.DOB</td>
                </tr>
            }
</table>

